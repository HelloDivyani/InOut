<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hackathon.example.paisa"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="8" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="LoginActivity"></activity>
        <activity android:name="RegisterActivity"></activity>
        <activity android:name="Profile"></activity><activity android:name="Paisa"></activity><activity android:name="Settings"></activity>
        
        <activity android:name="Messages"></activity>
        <activity android:name="Settings_Bank"></activity>
        <activity android:name="Settings_Wallet"></activity>
        <activity android:name="Search"></activity>
        
    </application>

</manifest>
